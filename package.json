{
  "name": "api-test-jest",
  "version": "1.0.0",
  "description": "API test with jest",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "test": "jest --config jest.config.ts",
    "test:health": "jest -t=health",
    "test:regression": "jest -t=regression",
    "test:retry": "jest -t=retry",
    "test:ignore": "jest --testPathIgnorePatterns=retry",
    "test:debug": "jest -i"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/k0libri/api-test-jest.git"
  },
  "keywords": [],
  "author": "k0libri",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/k0libri/api-test-jest/issues"
  },
  "homepage": "https://github.com/k0libri/api-test-jest#readme",
  "devDependencies": {
    "@types/jest": "^29.5.3",
    "@types/jest-json-schema": "^6.1.1",
    "jest": "^29.5.0",
    "jest-json-schema": "^6.1.0",
    "jest-stare": "^2.5.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6"
  },
  "dependencies": {
    "axios": "^1.2.2",
    "http-status-codes": "^2.2.0"
  }
}
